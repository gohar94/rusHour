<% @page_title = "RusHour | Add Service" %>
<%= link_to("<< Back", {:action => 'index'}, :class => 'back-link') %>

<div class="services new">
  <h2>Create Service</h2>

  <%= form_for(:service, :url => {:action => 'create'}) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Create Service") %>
    </div>

  <% end %>
</div>